    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ModularMonolith.Host/bin/Debug/net9.0/ModularMonolith.Host.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ModularMonolith.Host",
            "stopAtEntry": false,
            "console": "internalConsole",
